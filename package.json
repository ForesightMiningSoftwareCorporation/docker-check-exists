{
  "name": "docker-check-exists",
  "version": "1.0.0",
  "description": "Github Action to check if a docker image exists in a Docker Registry",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ForesightMiningSoftwareCorporation/docker-check-exists.git"
  },
  "keywords": [],
  "author": "Lo√Øs Postula <lois@postu.la>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "got": "^12.3.1"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/node": "^18.14.4",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "typescript": "^4.4.4"
  }
}
